{
  "name": "notabug-peer",
  "version": "0.8.11",
  "description": "notabug network peer",
  "main": "./index.js",
  "scripts": {
    "prepublish": "npm run build-all",
    "preversion": "npm run build-all && npm run unit",
    "version": "git add .",
    "postversion": "git push && git push --tags",
    "build": "gulp build",
    "build-dev": "gulp build-dev",
    "build-web": "gulp build-web",
    "build-web-dev": "gulp build-web-dev",
    "build-all": "gulp",
    "unit": "mocha --compilers js:babel-core/register --colors ./test/*.spec.js",
    "unit-watch": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js",
    "test": "npm run unit-watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/notabugio/notabug-peer.git"
  },
  "keywords": [
    "es6",
    "npm",
    "starter",
    "library",
    "boilerplate",
    "babel",
    "umd",
    "webpack",
    "universal"
  ],
  "author": {
    "name": "go1dfish",
    "url": "https://github.com/go1dfish"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/notabugio/notabug-peer/issues"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "lib",
    "dist"
  ],
  "homepage": "https://notabug.io/",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.4.1",
    "clean-webpack-plugin": "^0.1.8",
    "del": "^2.2.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.16.1",
    "eslint-plugin-standard": "^1.3.1",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-env": "^0.2.0",
    "gulp-eslint": "^1.1.1",
    "gulp-load-plugins": "^1.2.0",
    "gulp-util": "^3.0.6",
    "mocha": "^2.3.4",
    "object-assign": "^4.0.1",
    "webpack": "^1.12.11",
    "webpack-node-externals": "^0.4.1"
  },
  "library": {
    "name": "notabug-peer",
    "entry": "notabug.js",
    "dist-node": "notabug.js",
    "dist-web": "notabug.web.js",
    "bundle-node": false
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.2",
    "babel-preset-stage-0": "^6.24.1",
    "lodash": "^4.17.10",
    "proof-of-work": "https://github.com/notabugio/proof-of-work.git#build-compat",
    "ramda": "^0.25.0",
    "route-parser": "^0.0.5",
    "text-encoding": "^0.6.4",
    "urllite": "^0.5.0"
  }
}
