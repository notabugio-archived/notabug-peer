{
  "name": "notabug-peer",
  "version": "0.12.4",
  "description": "Peer/client library for notabug network",
  "main": "lib/notabug-peer.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-register --colors -w ./test/*.spec.js",
    "test:cover": "cross-env NODE_ENV=test nyc mocha --require babel-register --colors test/*.js",
    "repl": "node -i -e \"$(< ./lib/notabug-peer.js)\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/notabugio/notabug-peer.git"
  },
  "keywords": ["library", "notabug", "gundb"],
  "author": "go1dfish",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/notabugio/notabug-peer/issues"
  },
  "homepage": "https://github.com/notabugio/notabug-peer",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@trust/webcrypto": "^0.9.2",
    "argon2": "0.20.1",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-env": "^7.0.0-beta.3",
    "babel-register": "^7.0.0-beta.3",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "gun-scope": "https://github.com/notabugio/gun-scope.git",
    "gun-suppressor": "https://github.com/notabugio/gun-suppressor.git",
    "gun-suppressor-sear": "https://github.com/notabugio/gun-suppressor-sear.git",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "mocha": "^4.0.1",
    "node-webcrypto-ossl": "^1.0.42",
    "nyc": "^13.1.0",
    "object-hash": "^1.1.4",
    "query-string": "^6.4.2",
